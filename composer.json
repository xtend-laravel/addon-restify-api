{
    "name": "xtend-laravel/addon-restify-api",
    "description": "Setup Restify API for Lunar",
    "type": "xtend-addon",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "binaryk/laravel-restify": "dev-custom-namespace-support"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adam-code-labx/laravel-restify.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "XtendLunar\\Addons\\RestifyApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adam Lee",
            "email": "adam@codelabx.ltd"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "XtendLunar\\Addons\\RestifyApi\\RestifyApiProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
